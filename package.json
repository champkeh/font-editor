{
  "name": "font-editor",
  "version": "0.0.2",
  "description": "a font-editor written with JavaScript and no dependency.",
  "scripts": {
    "bundle": "rollup -c",
    "typings": "tsc -d --emitDeclarationOnly",
    "build": "npm run typings && npm run bundle",
    "check-types": "tsc -noEmit",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/champkeh/font-editor.git"
  },
  "keywords": [
    "font-editor",
    "font-parser",
    "ttf"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.iife.js",
  "types": "typings/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "author": "champion.ge@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/champkeh/font-editor/issues"
  },
  "homepage": "https://github.com/champkeh/font-editor#readme",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^17.0.33",
    "font-carrier": "^0.3.1",
    "mkdirp": "^1.0.4",
    "rollup": "^2.74.1",
    "rrweb": "^1.1.3",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
